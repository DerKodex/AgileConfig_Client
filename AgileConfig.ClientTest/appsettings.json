{
  "AgileConfig": {
    "appId": "test_app",
    "secret": "test_app",
    "nodes": "http://agileconfig_server.xbaby.xyz/",
    //"nodes": "http://localhost:5000",
    "name": "哈哈",
    "tag": "tag1",
    "env": "DEV",
    "httpTimeout": "10", //s
    "cache": { // 指定缓存文件的目录
      "directory": "c:/data"
    },
    "serviceRegister": { //服务注册信息
      "serviceId": "test_app_service_01", //服务id，全局唯一，用来唯一标示某个服务
      "serviceName": "test_app", //服务名，可以重复，某个服务多实例部署的时候这个serviceName就可以重复
      "ip": "127.0.0.1", //服务的ip
      "port": 5001, //服务的端口
      "metaData": [
        "my name is MJ",
        "ECS=001",
        "v1"
      ],
      "check": {
        "url": "http://www.baidu.com", //健康检测地址，如果是httpstatus=200则判定存活，其它都视为失败
        "interval": "30" //s
      }
    }
  }
}